{
    "schemaVersion": 1,
    "id": "arcanumapi",
    "version": "${version}",
  
    "name": "Arcanum API",
    "description": "An X-plat API for presentable, data-driven mod documentation based on the Arcana project. \nFabric build.",
    "authors": [
      "Falkory", "Macy"
    ],
    "contributors": ["Jared"],
    "contact": {
      "homepage": "https://falkory220.github.io/ArcanumAPI/",
      "sources": "https://github.com/Falkory220/ArcanumAPI",
      "issues": "https://github.com/Falkory220/ArcanumAPI/issues",
      "discord": "discord.gg/YtVnxw43ew"
    },
    "custom": {
      "modmenu": {
        "badges": ["library"]
      }
    },
  
    "license": "MIT",
    "icon": "icon_fabric.png",
  
    "environment": "*",
